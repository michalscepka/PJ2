		<CheckBox x:Name="overclock_chb" Content="Overclock" HorizontalAlignment="Left" Margin="329,42,0,0" VerticalAlignment="Top"/>
		<TextBox x:Name="id_tb" HorizontalAlignment="Left" Margin="463,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="34" Height="20"/>
		<Label Content="Data for Add/Edit: Name, Price, Cores, Frequency, Architecture" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
		<Button x:Name="button_remove_Copy1" Content="Save" HorizontalAlignment="Left" Margin="722,40,0,0" VerticalAlignment="Top" Width="50" Click="Load_Click"/>

