		<Label x:Name="price_label" Content="prices" HorizontalAlignment="Left" Margin="182,101,0,0" VerticalAlignment="Top"/>
		<Label x:Name="cores_label" Content="cores" HorizontalAlignment="Left" Margin="267,101,0,0" VerticalAlignment="Top"/>
		<Label x:Name="freq_label" Content="freqs" HorizontalAlignment="Left" Margin="312,101,0,0" VerticalAlignment="Top"/>
		<Label x:Name="arch_label" Content="achrs" HorizontalAlignment="Left" Margin="387,101,0,0" VerticalAlignment="Top"/>
		<Label x:Name="overclock_label" Content="overs" HorizontalAlignment="Left" Margin="517,101,0,0" VerticalAlignment="Top"/>
		<TextBox x:Name="name_tb" HorizontalAlignment="Left" Margin="14,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="90"/>
		<TextBox x:Name="price_tb" HorizontalAlignment="Left" Margin="109,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="50"/>
