		<Label Content="Name" HorizontalAlignment="Left" Margin="47,71,0,0" VerticalAlignment="Top"/>
		<Label Content="Price" HorizontalAlignment="Left" Margin="182,71,0,0" VerticalAlignment="Top"/>
		<Label Content="Cores" HorizontalAlignment="Left" Margin="267,71,0,0" VerticalAlignment="Top"/>
		<Label Content="Frequency" HorizontalAlignment="Left" Margin="312,71,0,0" VerticalAlignment="Top"/>
		<Label Content="Architecture" HorizontalAlignment="Left" Margin="387,71,0,0" VerticalAlignment="Top"/>
